wpscan --url $URL -e p,u --plugins-detection aggressive

wpscan --url $URL -e ap  --plugins-detection aggressive --api-token [redacted]

wpscan --url $URL -e ap  --plugins-detection aggressive

wpscan --url $URL -e ap --detection-mode aggressive 

wpscan --url $URL -e ap --log wpscan.out

wpscan --url $URL --disable-tls-checks --enumerate p --enumerate t --enumerate u

wpscan --url $URL --enumerate at --enumerate ap --enumerate u

wpscan --url $URL --enumarate -u,ap,tt,t --log `pwd`/$ip_wpscan.log

wpscan --url $URL --enumerate upt

wpscan --url $URL -e vt,tt,u,ap --log wpscan.log

wpscan --url $URL --wp-content-dir -at -ap -eu

wpscan --url $URL --wordlist `pwd`/list.txt --username $username

wpscan --url $URL --wordlist <wordlist> --username $username

wpscan --url $URL -U <USERSFILE> -P <PASSWORDFILE>

wpscan --url $URL -U user.txt -P /usr/share/wordlists/rockyou.txt

wpscan --url $URL --username $username --wordlist `pwd`/passwords

wpscan --url $URL --enumarate p,t,u,tt | tee wpscan.log

wpscan --url http://127.0.0.1:1337/wordpress --proxy $URL:$PORT -e u
